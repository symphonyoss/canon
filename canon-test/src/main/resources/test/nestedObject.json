  "x-canon-id": "https://github.com/bruceskingle/canon/blob/master/canon-test/src/main/resources/test/nestedObject.json",
  "x-canon-model": {
    "javaGenPackage":  "org.symphonyoss.s2.canon.test.unit",
    "javaFacadePackage":  "org.symphonyoss.s2.canon.test.unit.facade"
  },
  "paths": {
    "/nestedObject": {
      "get": {
        "description": "Returns a nested object",
        "parameters": [
          {
            "name": "request",
            "in": "query",
            "description": "Request params",
            "required": false,
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "petType": {
                  "type": "string"
                }
              }
            }
          },
          {
            "name": "count",
            "in": "query",
            "description": "Request count",
            "required": false,
            "schema": {
              "$ref": "#/components/schemas/Int64Max"
            }
          }
        ],
        "responses": {
          "200": {          
            "description": "A list of objects.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "required": [
                      "outerNestedObject"
                    ],
                    "properties": {
                      "yetAnotherBoolean": {
                        "description": "A boolean.",
                        "type": "boolean"
                      },
                      "outerNestedObject": {
                        "type": "object",
                        "required": [
                          "innerNestedObject"
                        ],
                        "properties": {
                          "outerName": {
                            "type": "string"
                          },
                          "outerValue": {
                            "description": "A double with limits",
                            "type": "number",
                            "format": "double",
                            "minimum": -765546546547723.03330025,
                            "maximum": 7665465456464550000.00333025
                          },
                          "innerNestedObject": {
                            "type": "object",
                            "properties": {
                              "innerName": {
                                "type": "string"
                              },
                              "innerValueFloat": {
                                "description": "A float",
                                "type": "number",
                                "format": "float",
                                "minimum": -77723.00025,
                                "maximum": 7650000.00025
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Int64Max": {
        "description": "A 64 bit integer with a maximum.",
        "type": "integer",
        "format": "int64",
        "maximum": 80000
      }
    }
  }
}